{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "RvDADF"
		},
		"ADLStest_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLStest'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzureSqlDatabase2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase2'"
		},
		"AzureSynapseAnalytics1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSynapseAnalytics1'"
		},
		"BlobRemco_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'BlobRemco'"
		},
		"EPDB2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'EPDB2'"
		},
		"FtpServerMirek_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FtpServerMirek'"
		},
		"Oracle1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Oracle1'"
		},
		"SQLParamLS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLParamLS'"
		},
		"elasticpool_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'elasticpool'"
		},
		"linkedService1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedService1'"
		},
		"rvdsynapseadls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'rvdsynapseadls'"
		},
		"ADLStest_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://rvdsynapsealds.dfs.core.windows.net"
		},
		"FtpServerMirek_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "ftp.kpn.com"
		},
		"FtpServerMirek_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "dgkdhjf"
		},
		"rvdsynapseadls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://rvdsynapsealds.dfs.core.windows.net"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "RvDADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLStest_accountKey": {
						"value": "[parameters('ADLStest_accountKey')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzureSqlDatabase2_connectionString": {
						"value": "[parameters('AzureSqlDatabase2_connectionString')]"
					},
					"AzureSynapseAnalytics1_connectionString": {
						"value": "[parameters('AzureSynapseAnalytics1_connectionString')]"
					},
					"BlobRemco_connectionString": {
						"value": "[parameters('BlobRemco_connectionString')]"
					},
					"EPDB2_connectionString": {
						"value": "[parameters('EPDB2_connectionString')]"
					},
					"FtpServerMirek_password": {
						"value": "[parameters('FtpServerMirek_password')]"
					},
					"Oracle1_connectionString": {
						"value": "[parameters('Oracle1_connectionString')]"
					},
					"SQLParamLS_connectionString": {
						"value": "[parameters('SQLParamLS_connectionString')]"
					},
					"elasticpool_connectionString": {
						"value": "[parameters('elasticpool_connectionString')]"
					},
					"linkedService1_connectionString": {
						"value": "[parameters('linkedService1_connectionString')]"
					},
					"rvdsynapseadls_accountKey": {
						"value": "[parameters('rvdsynapseadls_accountKey')]"
					},
					"ADLStest_properties_typeProperties_url": {
						"value": "[parameters('ADLStest_properties_typeProperties_url')]"
					},
					"FtpServerMirek_properties_typeProperties_host": {
						"value": "[parameters('FtpServerMirek_properties_typeProperties_host')]"
					},
					"FtpServerMirek_properties_typeProperties_userName": {
						"value": "[parameters('FtpServerMirek_properties_typeProperties_userName')]"
					},
					"rvdsynapseadls_properties_typeProperties_url": {
						"value": "[parameters('rvdsynapseadls_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "RvDADF_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'RvDADF_ArmTemplate_0')]"
			]
		},
		{
			"name": "RvDADF_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'RvDADF_ArmTemplate_1')]"
			]
		}
	]
}